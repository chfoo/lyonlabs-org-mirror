READY.10 USR $05AB20 TO $600030 @ "SETUP"40 LDA #$0050 STA $3460 LDA #$D470 STA $3580 @ "CHEK"90 INC $35100 @ "CHEK2"110 LDY #$18120 @ "NEXT"200 LDA ($34),Y210 BNE $"NEXT1"220 DEY230 BNE $"NEXT"240 LDA #$41250 LDY #$12260 STA ($34),Y270 LDY #$1B280 LDA ($34),Y290 CMP #$FF300 BNE $"FINSH"301 TAY302 TAX304 @ "DELAY"305 DEX306 BNE $"DELAY"307 DEY308 BNE $"DELAY"310 LDA $35320 RTS330 @ "NEXT1"340 LDA $35350 CMP #$DF360 BNE $"CHEK"365 @ "FINSH"370 LDA #$00380 RTS390 @ "START"400 JSR $"SETUP"410 JSR $"CHEK2"420 BEQ $"SID"430 TAY440 DEY450 DEY460 STY $467F470 INY480 TYA490 INY500 JSR $"ADD"510 STA $2670520 TYA530 JSR $"ADD"540 STA $2676550 LDA #$02560 @ "SID"570 STA $5C71580 RTS590 @ "ADD"600 AND #$0F610 ORA #$30620 CMP #$3A630 BCC $"RTS"640 ADC #$86650 @ "RTS"660 RTS670 ASC "ÒAY"READY.